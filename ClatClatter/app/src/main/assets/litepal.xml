<?xml version="1.0" encoding="utf-8"?>
<!-- 李世杰创建，用于配置litepal的文件 -->
<litepal>
    <!-- 配置数据库的名称 -->
    <dbname value="TaskManager"></dbname>
    <!-- 数据库的版本，用于数据库更新的时候使用 -->
    <version value="1"></version>

    <!-- 用于指定所有数据库中的所有映射 -->
    <list>
        <mapping class="com.jacklee.clatclatter.database.task"></mapping>
        <mapping class="com.jacklee.clatclatter.database.app_backlist"></mapping>
    </list>
</litepal>